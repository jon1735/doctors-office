<h1><%= @patient.name %></h1>
<br>
  <ul>
    <%@patient.doctors.each do |doctor|%>
      <li>
        <%= doctor.name %>
        <% doctor.appointments.where(patient_id: @patient.id).each do |appointment| %>
            <%= appointment.date %>
            <%= appointment.notes %>
        <% end %>
      </li>
    <% end %>
</ul>